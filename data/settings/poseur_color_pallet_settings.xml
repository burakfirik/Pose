<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<color_pallet>
    <pallet_size>20</pallet_size>
    <pallet_rows>2</pallet_rows>
    <default_color><red>200</red><green>200</green><blue>200</blue></default_color> <!-- LIGHT GRAY -->
    <pallet_color><red>0</red><green>0</green><blue>0</blue></pallet_color>         <!-- BLACK -->
